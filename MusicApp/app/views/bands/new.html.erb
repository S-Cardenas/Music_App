<h1>New Band</h1>
<%= flash[:errors] %>
<form action="/bands" method="POST">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">
  <label>Band Name</label>
  <input type="text" name="band[name]">
  <br>
  <input type="submit" value="Register Band!!">
</form>
